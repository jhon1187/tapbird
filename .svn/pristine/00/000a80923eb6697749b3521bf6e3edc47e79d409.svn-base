echo off
setlocal enabledelayedexpansion
rmdir /s /q assets
mkdir assets
for /f %%f in ('dir /b /ad ..\desenv') do (
	if NOT %%f == scripts (
		mklink /J assets\%%f ..\desenv\%%f
	)
)
for /f %%f in ('dir /b /a-d ..\desenv') do (
	set "N=%%f"
	if "!N:.exe=!"=="!N!" (
		if "!N:.dll=!"=="!N!" (
			if "!N:.angelscript=!"=="!N!" (
				if "!N:.log.txt=!"=="!N!" (
					if "!N:.bin=!"=="!N!" (
						mklink /H assets\%%f ..\desenv\%%f
					)
				)
			)
		)
	)
)

mklink /H assets\android_game.bin ..\desenv\game.bin

copy /Y ..\lib\libApplication.so_ libs\armeabi\libApplication.so